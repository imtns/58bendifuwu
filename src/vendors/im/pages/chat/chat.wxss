@import "./../../global/common.wxss";
/**chat.wxml**/
  page{
    overflow:hidden;
  }

    #captcha {
        top: .385rem;
    }

  /**msglist**/
  .msg-mask{
    position:absolute;
    top:0;
    right:0;
    left:0;
    bottom:82rpx;
    z-index:999;
    background:#EBEBEB;
  }

  .msglist-mask{
      position:absolute;
      top:0;
      right:0;
      left:0;
      bottom:82rpx;
      z-index:999;
      background:#EBEBEB;
      overflow: hidden;
  }
  .msglist-mask-top{
      position: fixed;
      top:0;
      left:0;
      right:0;
      bottom:82rpx;
      background: transparent;
      z-index: 1000;
  }
     .im-msg-list {
            list-style: none;
            display: block;
            position: relative;
            padding: 0 0 82rpx 0;
            margin: 0;
            background-color: #EBEBEB;
            box-sizing:border-box
        }

        .im-msg {
            list-style: none;
            display: block;
            position: relative;
            margin-bottom: 72rpx;
        }

        .im-msg.im-msg-me {
            text-align: right;
        }

  /**msgitem.wxss**/
    .im-msg-text {
        position: relative;
    }

    .im-msg-time {
        text-align: center;
        margin-bottom: 24rpx;
    }

    .im-msg-time.im-msg-time-hide{
         display:none;
    }

    .im-msg-time .im-msg-time-content {
        display:inline-block;
        max-width: 50%;
        background: #CECECE;
        border-radius: 8rpx;
        font-size: 24rpx;
        color: #FFFFFF;
        letter-spacing: 0;
        padding: 8rpx 14rpx;
    }

    .im-msg .im-msg-avatar {
        position: absolute;
        width: 80rpx;
        height: 80rpx;
        border-radius: 50%;
        border: none;
        background-position: 50% 50%;
        background-repeat: no-repeat;
        background-attachment: scroll;
        background-size: cover;
        top: 0;
    }

    .im-msg.im-msg-other .im-msg-avatar {
        left: -100rpx;
    }

    .im-msg.im-msg-me .im-msg-avatar {
        right: -100rpx;
    }

    .im-msg-main {
        display: inline-block;
        max-width: 60%;
        position: relative;
        background: #FFFFFF;
        border-radius: 12rpx;
        padding: 16rpx;
    }

    .im-msg-main .im-msg-content {
        word-break: break-all;
        letter-spacing: 0;
        display: block;
        text-align: left;
    }

    .im-msg-main .im-msg-arrow {
        content: '';
        position: absolute;
        width:0;
        height:0;
        top:0;
        margin-top: 27rpx;
        border-top:10rpx solid transparent;
        border-bottom:10rpx solid transparent;
    }
    .im-msg.im-msg-me .im-msg-main .im-msg-arrow{
        right: -8rpx;
        border-left:10rpx solid #00BDFD;
    }
    .im-msg.im-msg-other .im-msg-main .im-msg-arrow{
        left: -8rpx;
        border-right:10rpx solid #FFFFFF;
    }

    .im-msg-main .im-msg-send-status {
        margin-left: .07rem;
        background-color: #f1f1f1;
        color: #000000;
        margin-right: .07rem;
        display: none;
    }

    .im-msg.im-msg-me .im-msg-main {
        background-color: #00BDFD;
        color: #ffffff;
    }

    .im-msg.im-msg-me .im-msg-main {
        margin-right: 120rpx;
    }

    .im-msg.im-msg-other .im-msg-main {
        margin-left: 120rpx;
    }

    .im-msg.im-msg-me.im-msg-sending .im-msg-send-status,
    .im-msg.im-msg-me.im-msg-fail .im-msg-send-status {
        content: '';
        position: absolute;
        top: 50%;
        width: 40rpx;
        height: 40rpx;
        right: 100%;
        margin-top: -20rpx;
        margin-right: 10rpx;
        display: inline-block;
    }

    .im-msg.im-msg-me.im-msg-fail .im-msg-send-status {
        background-image: url(data:image/png;base64,);
        background-size: contain;
        background-repeat: no-repeat;
    }

    .im-msg.im-msg-me.im-msg-sending .im-msg-send-status {
        background-image: url(data:image/png;base64,);
        background-size: contain;
        background-repeat: no-repeat;
    }

    .im-msg-feedback {
        display: none;
    }

    .im-msg-feedback .im-msg-feedback-container {
        position: relative;
        background: #CECECE;
        border-radius: 8rpx;
        padding: 8rpx 14rpx;
        color: #FFFFFF;
        display: inline-block;
        font-size: 24rpx;
        letter-spacing: 0;
    }

    .im-msg.im-msg-me.im-msg-fail .im-msg-feedback {
        display: block;
        text-align: center;
        margin-top: 8rpx;
    }

  /**editor.wxml**/
.im-editor {
    display: flex;
    flex-direction: row;
    padding: 8rpx 20rpx;
    background: #F4F4F6;
    border-top: 2rpx solid #DDDDDD;
    position: fixed;
    bottom: 0;
    right: 0;
    left: 0;
}

.im-input {
    height: 64rpx;
    background: #FCFCFC;
    border: 2rpx solid #DDDDDD;
    border-radius: 12rpx;
    outline: none;
    margin: 0 20rpx;
    flex-grow: 1;
}

.im-button {
    width: 160rpx;
    height: 64rpx;
    margin-left: 8rpx;
    outline: none;
    color: #FFFFFF;
    letter-spacing: 0;
    background: #00BDFD;
    border: 2rpx solid #DDDDDD;
    border-radius: 8rpx;
    font-size: 26rpx;
    flex-shrink: 0;
}



/** image **/
.im-msg-image .im-msg-main{
  padding: 0;
}

/** Tip **/
.im-msg-tip .im-msg-main {
  max-width: 100%;
  display: block;
  text-align: center;
  background-color: transparent !important;
  color: #ffffff;
}
.im-msg-tip-container {
  position: relative;
  border-radius: 8rpx;
  background: #CECECE;
  padding: 4rpx 10rpx;
  display: inline-block;
  font-size: 28rpx;
  letter-spacing: 0;
}

/** audio **/
.im-msg-audio .im-msg-main {
  max-width: 30%;
  padding: 24rpx;
}
.im-msg-audio .im-msg-content {
  width: 30rpx;
  height: 30rpx;
  background-size: cover;
  background-repeat: no-repeat;
}
.im-msg-audio .im-msg-duration {
  content: '';
  position: absolute;
  top: 50%;
  width: 30rpx;
  height: 26rpx;
  margin-top: -12rpx;
  display: inline-block;
  font-size: 26rpx;
}
.im-msg-audio .im-msg-read-status {
  position: absolute;
  width: 15rpx;
  height: 15rpx;
  top: -2rpx;
  border-radius: 50%;
  background: #F43531;
  right:-10rpx;
}
.im-msg-audio .im-msg-read-status.im-audio-read {
  display: none;
}
.im-msg-audio.im-msg-other .im-msg-main .im-msg-content {
  background-image: url('data:image/png;base64,');
  background-size: contain;
  background-repeat: no-repeat;
}
.im-msg-audio.im-msg-other .im-msg-main .im-msg-content.im-audio-playing {
  background-image: url('data:image/gif;base64,');
  background-size: contain;
  background-repeat: no-repeat;
}
.im-msg-audio.im-msg-other .im-msg-main .im-msg-duration {
  right: -42rpx;
}

.im-msg-audio.im-msg-me .im-msg-main .im-msg-content {
  float: right;
  background-image: url('data:image/png;base64,');
  background-size: contain;
  background-repeat: no-repeat;
}
.im-msg-audio.im-msg-me .im-msg-main .im-msg-content.im-audio-playing {
  background-image: url('data:image/gif;base64,');
  background-size: contain;
  background-repeat: no-repeat;
}
.im-msg-audio.im-msg-me .im-msg-main .im-msg-arrow {
  margin-left: 26rpx;
}
.im-msg-audio.im-msg-me .im-msg-main .im-msg-duration {
  color: #2c3e50;
  left: -42rpx;
}
.im-msg-audio.im-msg-me .im-msg-main .im-msg-read-status {
  display: none;
}

  /**captcha**/
  .im-captcha {
      position:fixed;
      z-index:100;
      width:100%;
      height:100%;
      top:0;
      left:0;
  }

  .im-captcha.im-captcha-hide{
     display: none;
  }

  .im-captcha-mask {
      position: absolute;
      left: 0;
      top: 0;
      width: 100%;
      height: 100%;
      z-index: 99;
      background-color: rgba(0, 0, 0, 0.4);
  }

  .im-captcha-content {
      width:500rpx;
      height:300rpx;
      position:fixed;
      background-color:#fff;
      z-index:100;
      box-sizing:border-box;
      top:30%;
      left:50%;
      margin:-150rpx 0 0 -250rpx;
  }

  .im-captcha-tips {
      margin:20rpx auto;
      font-size:40rpx;
      text-align:center;
  }

  .im-captcha-tips.im-captcha-error {
      color: #ff552e;
  }

  .im-captcha-input {
      margin:auto;
      background:#f6f6f6;
      border:none;
      position:relative;
      line-height:100rpx;
      font-size:0;
      height:100rpx;
      width:90%;
  }

  .captcha-input {
      width:100%;
      box-sizing:border-box;
      display:inline-block;
      line-height:80rpx;
      border:none;
      outline:none;
      background:#f6f6f6;
      font-size:40rpx;
      vertical-align:middle;
      padding:10rpx;
  }

  .captcha-image {
      width:160rpx;
      height:80rpx;
      position:absolute;
      right:10rpx;
      top:10rpx;
      z-index:1000;
  }

  .im-captcha-btn {
      font-size:0;
      border-top:2rpx solid #f6f6f6;
      position:absolute;
      width:100%;
      bottom:0;
      padding:20rpx 0;

  }

  .im-captcha-cancel, .im-captcha-submit {
      box-sizing:border-box;
      width:50%;
      font-size:40rpx;
      line-height:40rpx;
      text-align:center;
      margin:0;
      display:inline-block;
  }

  .im-captcha-cancel {
      border-right: 2rpx solid #f6f6f6;
  }


