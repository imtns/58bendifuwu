  /**mix-item.wxml**/
  .im-msg-mix .im-msg-main {
      text-decoration: none;
      min-width: 60%;
  }

  .im-msg-mix .im-msg-main .im-msg-content {
      overflow: hidden;
      position: relative;
  }

  .im-msg-mix .im-msg-main .im-msg-content .im-mix-content {
      box-sizing: border-box;
      width: 100%;
      position: relative;
      color: #333;
      text-decoration: none;
      display: block;
  }

  .im-msg-mix .im-msg-main .im-msg-content .im-mix-content .im-mix-title {
      font-size:50rpx;
      color:#333333;
      letter-spacing:0;
      line-height:1.5;
      /*padding: 0 0.4rem 0 0;*/
  }

  .im-msg-mix .im-msg-main .im-msg-content .im-mix-content .im-mix-detail {
      font-size:40rpx;
      color:#999999;
      letter-spacing:0;
      line-height:1.5;
      margin-top:16rpx;
      margin-bottom:14rpx;
  }

  .im-msg-mix .im-msg-main .im-msg-content .im-mix-content .im-mix-detail.im-mix-detail-hide {
      display: none;
  }

  .im-msg-mix .im-msg-main .im-msg-content .im-mix-content .im-mix-img {
      padding:10rpx 0;
  }

  .im-msg-mix .im-msg-main .im-msg-content .im-mix-content .im-mix-img image {
      width: 100%;
      height: 200rpx;
  }

  .im-msg-mix .im-msg-main .im-msg-content .im-mix-content .im-mix-img.im-mix-img-hide {
      display: none;
  }

  .im-msg-mix .im-msg-main .im-msg-content .im-mix-content .im-mix-more {
      border-top:0 solid #F3F3F3;
      position:relative;
      padding:14rpx 0;
      font-size:26rpx;
      color:#666666;
      letter-spacing:0;
      line-height:1;
  }

  .im-msg-mix .im-msg-main .im-msg-content .im-mix-content .im-mix-more .im-mix-more-icon {
      width:14rpx;
      height:26rpx;
      background-image: url('data:image/png;base64,');
      background-size: contain;
      background-repeat: no-repeat;
      position: absolute;
      right: 0;
      top:13rpx;
  }


   /* 头部帖子 */
  .topic{
    position: fixed;
    top:0;
    padding:10rpx;
    height:120rpx;
    right: 0;
    left: 0;
    background: #fff;
    z-index: 1000;
    box-sizing: border-box;
  }
  .topic > image{
    height:100rpx;
    width:143rpx;
    position:absolute;
    top:10rpx;
    left:10rpx;
  }
  .topic > .txt{
    padding-left:153rpx;
    height:50rpx;
    line-height:50rpx;
  }
  .topic > .title{
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    display:block;
  }
  .topic.nopic > .txt{
    padding-left:10rpx;
  }
  .paddingTop{
    padding-top:124rpx;
    background-color: #EBEBEB;
  }
  .extrainfo{
    color:red;
    font-size:28rpx;
  }


