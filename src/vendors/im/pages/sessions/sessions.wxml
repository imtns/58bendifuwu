<!--sessions.wxml-->
<import src="../../templates/session/list.wxml"/>
 <block wx:if="{{isLogin}}">
    <template is="session-list" data="{{sessionList: sessionList, loadStatus: loadStatus}}"/>
</block>
<block wx:else>
    <view class="loginFrame" bindtap="login"> 
        <view class="thumb">
            <image class="icon" src="../../../../image/empty.png"></image>
        </view>
        <view class="tip">您还没有登录</view>
        <view class="tip">无法同步对话消息哦</view>
        <view class="loginButton">点击登录</view>
    </view>
</block>