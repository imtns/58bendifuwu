<script>
import wepy from 'wepy';

import util from '../utils/util';
import { get } from '../utils/ajax';
import { track } from '../utils/track';

const im = require('../utils/IMInit.js');
const app = require('../app');

let timerId = null;
let urlMap = null; // 首次加载是否展示地图
let urlTel = null; // 首次加载是否展示电话弹窗
const host = 'https://xiaochengxu.58.com';
export default class Xiangqingye extends wepy.page {
  config = {
      enablePullDownRefresh: false,
      navigationBarTitleText: '商家信息详情',
  };
  /**
   * 页面的初始数据
   */
  data = {
      isQB: app.globalData.isQB,
      detailShow: true, // 控制服务描述展示
      imgShow: true, // 控制图片展示
      bgShow: true, // 控制背景展示
      apCurrent: 0, // 控制轮播的索引
      imgWidth: 0, // 轮播外层宽度
      infoTitle: '', // 标题
      infocontent: '', // 服务描述
      objectType: '', // 服务类型
      postDate: '', // 发布日期
      renzheng: {}, // 认证相关
      liulan: '666', // 浏览量
      commparamMap: '', // 地图数据
      otherEntity: {}, // 其他认证
      commercializationMap: {}, // 相册
      serviceRange: [], // 服务区域
      infoid: '',
      userId: '',
      city: '',
      call: {
          tick: 0,
          hidden: true,
      },
  };
  /**
   * 生命周期函数--监听页面加载
   */
  onLoad(options) {
      track('show', {
          cateid: options.fullpath,
          area: options.city,
          pagetype: `xingqingye_${options.cate2ListName}`, // 页面类型，没有置空【必填】
      });
      const that = this;
      urlMap = false;
      urlTel = false;
      if (options.map) {
          urlMap = true;
      }
      if (options.tel) {
          urlTel = true;
      }
      Object.assign(this, options);
      this.userId = '';
      this.listName = options.cate2ListName;
      this.openid = options.openid || '';
      const url = `${host}/${options.city}/${options.cate2ListName}/${
          options.infoid
      }x.shtml`;
      get(url, {}, (e, response) => {
          if (e) {
              console.log(e);
              return false;
          }
          if (
              response.commercializationMap &&
        response.commercializationMap.picSmallList.length > 0
          ) {
              this.imgWidth =
          (response.commercializationMap.picSmallList.length * 168) + 15;
          }
          Object.assign(this, response);
          this.extendMap = response.wltEntity ? response.wltEntity.extendMap : '';
          this.shoplogo = response.extendMap
              ? response.extendMap.shoplogo
              : 'https://img.58cdn.com.cn/olympia/img/shenghuo/final/default-shopimg.png';
          this.userId = response.uid;
          if (urlMap) {
              that.onmapFun();
          }
          if (urlTel) {
              that.call_tick(1);
          }
          this.$apply();
          return true;
      });
  }
  /**
   * 展示图片
   */
  showImg(e) {
      this.imgShow = false;
      this.bgShow = false;
      this.apCurrent = e.currentTarget.dataset.index;
  }
  /**
   * 隐藏
   */
  hiddenFun() {
      this.imgShow = true;
      this.bgShow = true;
      this.detailShow = true;
      this.apCurrent = 0;
  }
  /**
   * 微聊
   */
  goChat() {
      console.log('点击接入微聊');
      const options = {
          user_id: this.userId,
          user_source: 2,
          refer: {
              invitation: {
                  id: this.infoid, // 帖子id
                  rootcateid: this.listName, // 一级分类id(表现类别)
                  cateid: '', // 最细类别id(表现类别)
                  scene: '', // 聊天触发的场景(listing"为列表详情页，其余由业务侧自己定义)
                  role: 1, // 角色标识,"1"为发帖者(业务线统一都传"1")
                  refer_time: new Date().getTime(),
              },
          },
          router_type: 'navigateTo',
      };
      im.gotoChat(options);
  }
  IMsuccess() {}
  IMerror(err) {
      console.log(err.msg);
  }
  /**
   * 控制服务描述展示
   */
  detailShow() {
      if (this.detailShow) {
          this.detailShow = false;
      } else {
          this.detailShow = true;
      }
  }
  /**
   * 地图跳转
   */
  onmapFun() {
      const that = this;
      const x = Math.floor(that.commparamMap.latitude * 100000) / 100000;
      const y = Math.floor(that.commparamMap.longitude * 100000) / 100000;
      wx.openLocation({
          latitude: x,
          longitude: y,
          address: that.commparamMap['商家地址'],
          name: this.qiyeEntity.enterpriseName,
      });
  }
  /* eslint-disable camelcase */
  /* 打电话 s */
  call_tick(e) {
      const self = this;
      let data;
      const { position } = e.currentTarget.dataset;
      track('click', {
          clickTag: `xiangqingye_call_${position}`,
      });
      data = {
          sign: self.sign,
          infoId: self.infoid,
          source: 0,
          type: 0,
      };
      if (e === 1) {
          data = Object.assign(data, {
              openid: self.openid,
          });
      }
      wx.request({
          url: `${app.globalData.domain}/smallapp/common/link`,
          data: data,
          header: app.globalData.testHeader,
          success(res) {
              if (res.data.code === 0) {
                  self.call = util.constDeepMixin(self.call, {
                      teleNumber:
              parseInt(res.data.data[0]) + 1 ? res.data.data : '服务忙',
                      tick: 180,
                      hidden: false,
                  });
                  self.bgShow = false;
                  self.$apply();
                  self.callTimer();
              } else {
                  console.error('400电话异常\n', res);
              }
          },
      });
  }
  callTimer() {
      clearInterval(timerId);
      timerId = setInterval(() => {
          if (this.call.tick > 1) {
              this.call = util.constDeepMixin(this.call, {
                  tick: --this.call.tick,
              });
              this.$apply();
          } else {
              clearInterval(timerId);
              this.call = util.constDeepMixin(this.call, {
                  tick: 0,
                  hidden: true,
              });
              this.bgShow = true;
              this.$apply();
          }
      }, 1000);
  }
  /* eslint-disable camelcase */
  cancel_call() {
      const self = this;
      clearInterval(timerId);
      this.call = util.constDeepMixin(self.call, {
          tick: 0,
          hidden: true,
      });
      this.bgShow = true;
  }
  call() {
      const self = this;
      wx.makePhoneCall({
          phoneNumber: self.call.teleNumber,
      });
  }
  /* 打电话 e */
  // 分享
  onShareAppMessage() {}
}
</script>
